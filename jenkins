pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello Baby how do you do'
            }
        }
        stage('Build') {
            steps {
                echo 'Building  a real pipeline'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Project'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
